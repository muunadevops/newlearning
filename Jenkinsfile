    agent any
    stages('Build') {
        steps {
            echo "build"
            echo "test"
            echo "integration test"
        }
    }
    stages('Test') {
     steps {
         echo "build"
         echo "test"
         echo "integration test"
     }
    }
    stages('Integration Test'){
        steps {
            echo "build"
            echo "test"
            echo "integration test"
        }
    }
}